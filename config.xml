<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.bvaprintweb.bvawebapss"
        versionCode="10" 
        version   = "1.0.0">

    <!-- versionCode is optional and Android only -->

    <name>BVA Print Web</name>
    <description>BVA Print Web Apps</description>
    <author href="http://bvaprintweb.com" email="jun@bvaprintweb.com">Juniawan</author>
	<preference name="fullscreen" value="true" />
	<preference name="stay-in-webview" value="true" />
	<preference name="exit-on-suspend" value="true" />
	<preference name="splash-screen-duration" value="5000" />
	<icon src="assets/images/appicons/iphone4.png" />
	<icon src="assets/images/appicons/iphone.png" gap:platform="ios" width="57" height="57" />
	<icon src="assets/images/appicons/ipad.png" gap:platform="ios" width="72" height="72" />
	<icon src="assets/images/appicons/iphone4.png" gap:platform="ios" width="114" height="114" />
	<gap:splash src="assets/images/startup/splash.png" />
</widget>